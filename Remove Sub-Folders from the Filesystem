class Solution {
    public List<String> removeSubfolders(String[] folder) {
        Arrays.sort(folder);    
        List<String> list = new ArrayList();
        
        String str = folder[0];
        for(int i=1; i<folder.length; i++){
            
            if(!folder[i].startsWith(str+"/")) {
                list.add(str); 
                str = new String(folder[i]);
            }
        }
        list.add(str);
        return list;
    }
}
